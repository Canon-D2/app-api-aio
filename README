# APP-API-AIO

# DELETE ALL LIBRARY
pip freeze | ForEach-Object { pip uninstall -y $_ }

# DELETE ALL DOCKER
docker system prune -a --volumes --force
docker builder prune --all --force

# EXPORT ALL LIBRARY CURRENCE TO REQUIREMENTS.TXT
pip freeze > requirements.txt

# DOCKER BUILT AND START (INCLUDE REQUIREMENTS)
docker-compose build
docker-compose up --no-log-prefix -d

# ACCESS FOLDER DOCKER
docker exec -it app-api-aio-api-1 /bin/sh
cd /opt/python-projects/app/

# RABBITMQ NOT WORKING -> STOP -> START AGAIN
rabbitmq-plugins enable rabbitmq_management
net stop RabbitMQ && net start RabbitMQ

# URL REDIS CONNECT
redis://:APP-API-AIO@127.0.0.1:6379/0

# RUN PS1 BACKUP DB
.\dump_db.ps1

# CREATE CHANNEL AND GET CHANNEL ID BY
# -> /start bot, add bot in channel (set admin) -> send channel -> get id
https://api.telegram.org/bot<BOT_TOKEN>/getUpdates